SAMPLE="$1"

#Exibe hora da submiss√£o ao sbatch e informacoes adicionais
date 
echo Info-Preftch "$SAMPLE"

cd "$PROJECT_DIR"

time vdb-dump --info "$SAMPLE"

time prefetch -X 1000000000  "$SAMPLE" -O ./sra 

echo Validate 
date

time vdb-validate "./sra/${SAMPLE}.sra"

echo Final "$SAMPLE" validation
date

